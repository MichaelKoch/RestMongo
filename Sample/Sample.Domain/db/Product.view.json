{
  "create": "viewProduct",
  "viewOn": "ProductColorSize",
  "pipeline": [
    {
      "$group": {
        "_id": "$MaterialNumber",
        "MaterialNumber": {
          "$first": "$MaterialNumber"
        },
        "Color": {
          "$first": "$Color"
        },
        "MainProductGroup": {
          "$first": "$MainProductGroup"
        },
        "RetailProductGroup": {
          "$first": "$MainProductGroup"
        },
        "Brand": {
          "$first": "$Brand"
        },
        "Gender": {
