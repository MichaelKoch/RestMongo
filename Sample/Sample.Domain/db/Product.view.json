        },
        "Line": {
          "$first": "$Line"
        },
        "MaterialText": {
          "$first": "$MaterialText"
        },
        "FormMaterialNumber": {
          "$first": "$FormMaterialNumber"
        },
        "QualityMaterialNumber": {
          "$first": "$QualityMaterialNumber"
        }
      }
    }
  ]
